ilteredProducts((prev) =>\n        [...prev].sort((a, b) => a.price - b.price)\n      );\n    } else {\n      setFilteredProducts((prev) =>\n        [...prev].sort((a, b) => b.price - a.price)\n      );\n    }\n  }, [sort]);\n\n  return (\n    <Container>\n      {cat\n        ? filteredProducts.map((item) => <Product item={item} key={item.id} />)\n        : products\n            .slice(0, 8)\n            .map((item) => <Product item={item} key={item.id} />)}\n    </Container>\n  );\n};\n\nexport default Products;\n","/Users/zhengxuan/Documents/GitHub/shop_ecommerce/client/src/components/Categories.jsx",[],[],"/Users/zhengxuan/Documents/GitHub/shop_ecommerce/client/src/components/Annoucement.jsx",[],[],"/Users/zhengxuan/Documents/GitHub/shop_ecommerce/client/src/components/Slider.jsx",[],[],"/Users/zhengxuan/Documents/GitHub/shop_ecommerce/client/src/components/Navbar.jsx",[],[],"/Users/zhengxuan/Documents/GitHub/shop_ecommerce/client/src/data.js",[],[],"/Users/zhengxuan/Documents/GitHub/shop_ecommerce/client/src/components/CategoryItem.jsx",[],[],"/Users/zhengxuan/Documents/GitHub/shop_ecommerce/client/src/components/Product.jsx",[],[],{"ruleId":"206","severity":1,"message":"207","line":2,"column":8,"nodeType":"208","messageId":"209","endLine":2,"endColumn":16},{"ruleId":"206","severity":1,"message":"210","line":3,"column":8,"nodeType":"208","messageId":"209","endLine":3,"endColumn":13},{"ruleId":"206","severity":1,"message":"211","line":4,"column":8,"nodeType":"208","messageId":"209","endLine":4,"endColumn":12},{"ruleId":"206","severity":1,"message":"212","line":8,"column":8,"nodeType":"208","messageId":"209","endLine":8,"endColumn":19},{"ruleId":"206","severity":1,"message":"213","line":9,"column":8,"nodeType":"208","messageId":"209","endLine":9,"endColumn":15},{"ruleId":"206","severity":1,"message":"214","line":127,"column":7,"nodeType":"208","messageId":"209","endLine":127,"endColumn":23},{"ruleId":"206","severity":1,"message":"215","line":9,"column":3,"nodeType